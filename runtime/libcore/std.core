proc loaded_pointer_to_Bool (_ptr: loaded pointer) : eff loaded integer :=
  case _ptr of
    | Specified(ptr: pointer) =>
        let weak is_null: boolean = memop(PtrEq, ptr, NULL(void*)) in
        pure(Specified(if is_null then 0 else 1))
    | Unspecified(_: ctype) =>
